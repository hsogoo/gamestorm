<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMapConfig
        PUBLIC "-//ibatis.apache.org//DTD SQL Map Config 2.0//EN"
        "http://ibatis.apache.org/dtd/sql-map-config-2.dtd">
<sqlMapConfig>
	<settings cacheModelsEnabled="false" enhancementEnabled="true"
		lazyLoadingEnabled="true" maxRequests="64" maxSessions="32"
		maxTransactions="16" useStatementNamespaces="false" defaultStatementTimeout="60" />
	<!-- 请保持和文件顺序保持一致 -->
    
	<sqlMap resource="sqlmap/sqlmap_user.xml" />
	<sqlMap resource="sqlmap/sqlmap_game.xml" />
	<sqlMap resource="sqlmap/sqlmap_category.xml" />
	<sqlMap resource="sqlmap/sqlmap_game_category.xml" />
	<sqlMap resource="sqlmap/sqlmap_product.xml" />
	<sqlMap resource="sqlmap/sqlmap_order.xml" />
	<sqlMap resource="sqlmap/sqlmap_order_item.xml" />
	
</sqlMapConfig>
